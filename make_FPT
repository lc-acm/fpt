
prefix="/Users/glick/Desktop/fpt_test"

cp -r fpt/ $prefix/

cd $prefix/DotosC
./compile_dotos


cd $prefix/DotosGPP
./compile_dotos


cd $prefix/FileProcessors
./compile

echo -e "#!/bin/bash\nprefix='$prefix'\n$(cat $prefix/acom)" > $prefix/acom

chmod +x $prefix/acom

ln -sf  $prefix/acom  /usr/local/bin/acom


